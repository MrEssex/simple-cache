  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.2",
    "roave/security-advisories": "dev-master"
  },
  "provide": {
    "psr/simple-cache-implementation": "^1.0"
  },
  "autoload": {
    "psr-4": {
